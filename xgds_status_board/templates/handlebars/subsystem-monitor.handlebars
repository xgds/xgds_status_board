<div class="grid-stack-item" data-gs-x="0" data-gs-y="1"
	data-gs-width="2" data-gs-height="2">
	<div class="grid-stack-item-content">
		<h1>EV1</h1>
		<table>
			<thead>
				<th>Subsystem</th>
				<th>Elapsed Time</th>
			</thead>
			<tbody>
				{{#EV1}}
				<tr>
					<td class="left subsystemName">{{displayName}}</td>
					<td class="left status elapsedTime" id="EV1_{{name}}_status">{{elapsedTime}}</td>
				</tr>
				{{/EV1}}
			</tbody>
		</table>
	</div>
</div>

<div class="grid-stack-item" data-gs-x="2" data-gs-y="1"
	data-gs-width="2" data-gs-height="3">
	<div class="grid-stack-item-content">
		<h1>EV2</h1>
		<table>
			<thead>
				<th>Subsystem</th>
				<th>Elapsed Time</th>
			</thead>
			<tbody>
				{{#EV2}}
				<tr>
					<td class="left subsystemName">{{displayName}}</td>
					<td class="left status elapsedTime" id="EV2_{{name}}_status">{{elapsedTime}}</td>
				</tr>
				{{/EV2}}
			</tbody>
		</table>
	</div>
</div>

<div class="grid-stack-item" data-gs-x=8" data-gs-y="1"
	data-gs-width="2" data-gs-height="2">
	<div class="grid-stack-item-content">
		<h1>Field Services</h1>
		<table>
			<thead>
				<th>Subsystem</th>
				<th>Elapsed Time</th>
			</thead>
			<tbody>
				{{#field}}
				<tr>
					<td class="left subsystemName">{{displayName}}</td>
					<td class="left status elapsedTime" id="field_{{name}}_status">{{elapsedTime}}</td>
				</tr>
				{{/field}}
				<tr id="fieldLoadAverage">
					<td class="left oneMin status" style="border: 1px solid black;"></td>
					<td class="left fiveMin status" style="border: 1px solid black;"></td>
					<td class="left elapsedTime"></td>
				</tr>
			</tbody>
		</table>
	</div>
</div>

<div class="grid-stack-item" data-gs-x="8" data-gs-y="2"
	data-gs-width="2" data-gs-height="2">
	<div class="grid-stack-item-content">
		<h1>Basecamp Services</h1>
		<table>
			<thead>
				<th>Subsystem</th>
				<th>Elapsed Time</th>
			</thead>
			<tbody>
				{{#baseCamp}}
				<tr>
					<td class="left subsystemName">{{displayName}}</td>
					<td class="left status elapsedTime" id="baseCamp_{{name}}_status">{{elapsedTime}}</td>
				</tr>
				{{/baseCamp}}
				<tr id="basecampLoadAverage">
					<td class="left oneMin status" style="border: 1px solid black;"></td>
					<td class="left fiveMin status" style="border: 1px solid black;"></td>
					<td class="left elapsedTime"></td>
				</tr>
			</tbody>
		</table>
	</div>
</div>